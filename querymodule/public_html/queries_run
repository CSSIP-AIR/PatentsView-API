SELECT 1378496895, @row_number:=@row_number+1 as sequence, XX.XXid as patent_id FROM (SELECT distinct patent.patent_id as XXid FROM  patent  left outer join patent_assignee on patent.patent_id=patent_assignee.patent_id left outer join assignee ON patent_assignee.assignee_id=assignee.assignee_id left outer join location as locationA on patent_assignee.location_id=locationA.location_id  WHERE ((patent.date >= '2001-01-01') and match (patent.abstract) against ('international' in boolean mode) and (assignee.lastknown_country <> 'US'))  ORDER BY patent.patent_id  limit 100000) XX, (select @row_number:=0) temprownum  
